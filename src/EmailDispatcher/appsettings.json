{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "SmtpSettings": {
    "Host": "localhost",
    "Port": 1025,
    "DisconnectDelaySeconds": 30,
    "RetryAttempts": 3
  }
}